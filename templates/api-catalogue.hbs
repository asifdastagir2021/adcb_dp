{{> breadcrumb}}

<div class="site-content">
    <!-- start hero segment -->
    <div class="container container--small">
        <div class="center" id="ca-logo-name">
            <!-- Don't change the below class name. As same is using for fetching the logo from themes.-->
            <p class="page-title-small color-red">
                {{#is language "en"}}
                API CATALOGUE
                {{else}}
                كتيّب واجهة برمجة التطبيقات API
                {{/is}}
            </p>
            <h1>
                {{#is language "en"}}
                Discover the list of APIs available for you at this time with ADCB
                {{else}}
                اكتشف قائمة واجهة برمجة التطبيقات API التي يوفرها لك بنك أبوظبي التجاري
                {{/is}}
            </h1>
        </div>
    </div>


    <div class="o-comp c-hero-segment">
        <!-- start c-comp-content -->
        <div class="o-comp__content container">
            <!-- start hero-segment blocks -->
            <div class="c-hero-segment__wrapper" id="rewards">
                <div class="c-hero-segment__img -parallax" data-position="center" data-bleed="0"
                    data-natural-width="530" data-natural-height="390" data-parallax="scroll"
                    data-image-src="{{images}}/touch-points.jpg">
                </div>
                <div class="c-hero-segment__info">
                    <h3 class="c-hero-segment__title"><sup class="c-hero-segment__sup">
                            {{#is language "en"}}
                            SEGMENTS
                            {{else}}
                            القطاعات
                            {{/is}}
                        </sup>

                        {{#is language "en"}}
                        TouchPoints APIs
                        {{else}}
                        واجهة برمجة التطبيقات API لمكافآت نقاط تاتش بوينتس
                        {{/is}}

                    </h3>
                    <div class="c-hero-segment__cta"><a href="touchpoints-api.html" class="o-btn-white">
                            {{#is language "en"}}
                            View more
                            {{else}}
                            عرض المزيد
                            {{/is}}
                        </a>
                    </div>
                </div>
            </div>
            <!-- end hero-segment blocks -->
            <!-- start hero-segment blocks -->
            <div class="c-hero-segment__wrapper" id="events">
                <div class="c-hero-segment__img -parallax" data-position="center" data-bleed="0"
                    data-natural-width="530" data-natural-height="390" data-parallax="scroll"
                    data-image-src="{{images}}/virtual-account.jpg">
                </div>
                <div class="c-hero-segment__info">
                    <h3 class="c-hero-segment__title"><sup class="c-hero-segment__sup">
                            {{#is language "en"}}
                            SEGMENTS
                            {{else}}
                            القطاعات
                            {{/is}}
                        </sup>
                        {{#is language "en"}}
                        Virtual Account APIs
                        {{else}}
                        واجهات برمجة التطبيقات APIs المخصصة للحسابات الافتراضية
                        {{/is}}
                    </h3>
                    <div class="c-hero-segment__cta"><a href="virtual-account-apis.html" class="o-btn-white">
                            {{#is language "en"}}
                            View more
                            {{else}}
                            عرض المزيد
                            {{/is}}
                        </a>
                    </div>
                </div>
            </div>
            <!-- end hero-segment blocks -->
        </div>
        <!-- end c-comp-content -->
    </div>
    <!-- end hero segment-->

</div>


